					<label class="labelinscription" class="" for="mdp">Mot de passe</label>
					<input type="password" id="mdp" minlength="3" maxlength="20" required /><br />
					<label class="labelinscription" class="" for="avatar">Avatar</label>
					<input type="file" id="avatar" accept="image/*"/><br />
